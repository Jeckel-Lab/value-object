{
    "name": "jeckel-lab/value-object",
    "description": "Collection of common and reusable value objects in PHP",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "jeckel-lab/contract": ">=1.2"
    },
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "vimeo/psalm": "^4.6",
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.9",
        "phpspec/phpspec": "^7.0",
        "friends-of-phpspec/phpspec-code-coverage": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Mercier-Rojas",
            "email": "julien@jeckel-lab.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeckelLab\\ValueObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\JeckelLab\\ValueObject\\": "spec/"
        }
    },
    "minimum-stability": "stable"
}
