{
    "name": "jeckel-lab/value-object",
    "description": "Collection of common and reusable value objects in PHP",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-filter": "*",
        "jeckel-lab/contract": "dev-main"
    },
    "require-dev": {
        "phpro/grumphp": "^1.5",
        "vimeo/psalm": "^4.12",
        "phpstan/phpstan": "^1.2",
        "phpmd/phpmd": "^2.11",
        "squizlabs/php_codesniffer": "^3.6",
        "roave/security-advisories": "dev-latest",
        "maglnet/composer-require-checker": "^3||^4",
        "phpunit/phpunit": "^9.5",
        "infection/infection": "^0.25"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Mercier-Rojas",
            "email": "julien@jeckel-lab.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeckelLab\\ValueObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\JeckelLab\\ValueObject\\": "spec/"
        }
    },
    "minimum-stability": "stable"
}
